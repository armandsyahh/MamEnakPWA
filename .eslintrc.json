{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "max-len": ["error", 1000],
    "no-shadow": "error",
    "no-unused-expressions": "error",
    "no-await-in-loop": "error",
    "consistent-return": "error",
    "no-mixed-operators": "error",
    "no-unused-vars": "error",
    "no-console": 0
  }
}